    "version": "0.2.0",
    "configurations": [
        {
            "name": "Run model pipeline",
            "type": "python",
            "request": "launch",
            "env": {
                "PYTHONPATH": "${workspaceFolder}\\fpl"
            },
            "cwd": "${workspaceFolder}\\fpl",
            "program": "src\\fpl\\custom_run\\kedro_cli.py",
            "args": [
                "run",
                "--pipeline",
                "model_pipeline"
            ],
            "console": "integratedTerminal",
            "justMyCode": false,
        },
        {
            "name": "Run scraping pipeline",
            "type": "python",
            "request": "launch",
            "env": {
                "PYTHONPATH": "${workspaceFolder}\\fpl"
            },
            "cwd": "${workspaceFolder}\\fpl",
            "program": "src\\fpl\\custom_run\\kedro_cli.py",
            "args": [
                "run",
                "--pipeline",
                "scraping_pipeline"
            ],
            "console": "integratedTerminal",
            "justMyCode": false,
        },
        {
            "name": "Run Init DB pipeline",
            "type": "python",
            "request": "launch",
            "env": {
                "PYTHONPATH": "${workspaceFolder}\\fpl"
            },
            "cwd": "${workspaceFolder}\\fpl",
            "program": "src\\fpl\\custom_run\\kedro_cli.py",
            "args": [
                "run",
                "--pipeline",
                "init_db_pipeline"
            ],
            "console": "integratedTerminal",
            "justMyCode": false,
        },
        {
            "name": "current file",
            "type": "python",
            "request": "launch",
            "env": {
                "PYTHONPATH": "${workspaceFolder}\\fpl"
            },
            "cwd": "${workspaceFolder}\\fpl",
            "program": "${file}",
            "justMyCode": false,
            "console": "integratedTerminal",
        }
    ]
}